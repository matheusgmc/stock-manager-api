{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "repositories/*": ["repositories/*"],
      "entities/index.ts": ["entities/index.ts"],
      "database/entities/*": ["database/entities/*"],
      "database/implements/*": ["database/implements/*"]
    },
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictPropertyInitialization": true,
    "skipLibCheck": true
  }
}
